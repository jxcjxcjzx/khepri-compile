/**
 * @fileOverview Khepri source optimization compiler stage.
 */
package (
    optimize)
with
    import '../khepri_peep' khepri_peep,
    import 'bes::object' {setProperty}
in {

/**
 * 
 */
optimize = \input#{ast data} ->
    setProperty(
        input,
        'ast',
        khepri_peep.optimize(ast, data));

}