/**
 * @fileOverview Binding record.
 */
package (
    Binding
    
    IMMUTABLE
    MUTABLE)
with
    import 'bes::record' record
in {

/**
 * Binding record.
 * 
 * @member value Value bound.
 * @member immutable Is the binding immutable?
 * @member simple is the bound value simple?
 */
Binding := record.declare(null, [
    'value',
    'immutable',
    'simple']);

IMMUTABLE := true;
MUTABLE := false;

}