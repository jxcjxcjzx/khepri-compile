/**
 * @fileOverview Khepri to ECMAScript translation stage.
 */
package transform
with 
    import '../transform/transform' transformer,
    import 'bes::object' {setProperty}
in {


transform = \input#{options tree data} ->
    setProperty(
        input,
        'tree',
        transformer.transform(
            tree,
            (options.package_manager || 'amd'),
            data));

}