/**
 * @fileOverview
 */
package optimize
with
    import 'akh::error' Error,
    
    import 'bes::object' {setProperty},
    
    import 'neith::tree' {node},

    import '../ecma_peep' ecma_peep
in {

/**
 * 
 */
optimize = \data#{tree} ->
    Error.of <| setProperty(
        data,
        'tree',
        ecma_peep.optimize(tree) |> node);

}