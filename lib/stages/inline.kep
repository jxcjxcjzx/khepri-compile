/**
 * @fileOverview Khepri code inlining compiler stage.
 */
package optimize
with
    import '../inline' inline,
    import 'bes::object' {setProperty}
in {

/**
 * 
 */
optimize = \input#{ast data} -> let
    out = inline.optimize(ast, data)
in
    setProperty(
        setProperty(input, 'tree', out.tree),
        'data',
        out.data);

}