{
    "name": "khepri-compile",
    "version": "0.3.0",
    "description": "Compiler for the Khepri programming language",
    "author": "Matt Bierner",
    "license": "MIT",
    "homepage": "http://khepri-lang.com",
    "keywords": [
        "Khepri",
        "ecmascript",
        "language",
        "programming language"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/mattbierner/khepri-compile.git"
    },
    "bugs": {
       "url" : "https://github.com/mattbierner/khepri/issues"
    },
    
    "main": "index.js",
    "directories": {
        "lib": "dist_node/"
    },
    "files": [
        "dist_node/",
        "lib/",
        "tests/",
        "index.js",
        "test.js"
    ],
    "dependencies": {
        "bes": "3.1.x",
        
        "neith": "1.5.x",
        
        "ecma-ast": "2.4.x",
        "ecma-ast-zipper": "1.0.x",
        
        "khepri-ast": "3.7.x",
        "khepri-ast-zipper": "1.1.x",
        
        "hashtrie": "0.2.x",
        
        "akh": "0.6.x",
        
        "require-directory": "1.2.x"
    },
    "devDependencies": {
        "nodeunit": "0.8.x",
        
        "khepri-parse": "0.9.x",
        "ecma-unparse": "3.2.x"
    },
    
    "scripts": {
        "test": "node test.js",
        
        "build": "npm run build-amd && npm run build-node",
        "build-amd": "khepri lib -o dist",
        "build-node": "khepri lib -o dist_node --package_manager=node"
    }
}
